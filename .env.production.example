# Production Security Environment Variables
# Copy to .env.production for production builds

# API Configuration
VITE_API_URL=https://api.orbittrails.com

# Security Settings
VITE_NODE_ENV=production
VITE_DISABLE_DEVTOOLS=true
VITE_HIDE_API_ERRORS=true

# Rate Limiting
VITE_RATE_LIMIT_ENABLED=true
VITE_MAX_REQUESTS_PER_MINUTE=100

# Security Headers
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true

# Feature Flags
VITE_DISABLE_CONSOLE_LOGS=true
VITE_ENABLE_REQUEST_OBFUSCATION=true
